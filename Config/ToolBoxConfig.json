{
    "Version": "1.0.0",
    "Application": {
        "Name": "PowerShell Admin ToolBox",
        "RequiredPowerShellVersion": "7.5",
        "RequiredDotNetVersion": "9.0",
        "Language": "fr-FR",
        "Theme": "Default"
    },
    "Logging": {
        "Level": "Info",
        "EnableConsoleLog": true,
        "EnableFileLog": true,
        "LogRetentionDays": 30,
        "LogFilePath": "./Logs"
    },
    "Authentication": {
        "GraphScopes": [
        "User.Read.All",
        "Group.Read.All",
        "Directory.Read.All"
        ],
        "SharePointTenantUrl": "",
        "UseCertificateAuth": false,
        "CertificateThumbprint": ""
    },
    "Modules": {
        "EnabledModules": [],
        "ModulesPath": "./Modules",
        "AutoDiscovery": true
    }
}